      '0':
        'name': 'punctuation.definition.string.begin.felix'
    'end': '"'
    'endCaptures':
      '0':
        'name': 'punctuation.definition.string.end.felix'
    'name': 'string.quoted.single.felix'
  }
  {
    'begin': '"""'
    'beginCaptures':
      '0':
        'name': 'punctuation.definition.string.begin.felix'
    'end': '"""'
    'endCaptures':
      '0':
        'name': 'punctuation.definition.string.end.felix'
    'name': 'string.quoted.other.multiline.felix'
  }
  {
    'begin': '/\\*'
    'beginCaptures':
      '0':
        'name': 'punctuation.definition.comment.begin.felix'
    'end': '\\*/'
